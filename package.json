{
  "name": "data-provider-nestjsx-crud",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && refine build",
    "dev": "refine dev",
    "refine": "refine",
    "start": "refine start --port 10000 --host 0.0.0.0",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "docker:build": "docker build -t motobike-rescue-admin .",
    "docker:run": "docker run -p 5173:10000 motobike-rescue-admin",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test:husky": "node scripts/test-husky.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx eslint --fix",
      "npx prettier --write"
    ],
    "*.{json,md}": [
      "npx prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@radix-ui/themes": "^3.2.1",
    "@refinedev/antd": "^5.45.3",
    "@refinedev/cli": "^2.16.44",
    "@refinedev/core": "^4.57.7",
    "@refinedev/kbar": "^1.3.16",
    "@refinedev/nestjsx-crud": "^5.0.12",
    "@refinedev/react-router": "^1.0.1",
    "@uiw/react-md-editor": "^3.19.5",
    "@xyflow/react": "^12.6.0",
    "antd": "^5.17.0",
    "buffer": "^6.0.3",
    "casbin": "^5.38.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.487.0",
    "react": "^18.0.0",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-router": "^7.0.2",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@iconify/react": "^5.2.0",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^18.16.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  }
}
